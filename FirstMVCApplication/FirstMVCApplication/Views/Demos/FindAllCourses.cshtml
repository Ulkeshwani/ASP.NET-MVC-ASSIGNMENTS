@model IEnumerable<FirstMVCApplication.Models.Course>

@{
    ViewBag.Title = "FindAllCourses";
}

<h2>FindAllCourses</h2>
<h2>Your Enrolled Courses</h2>
<h3> Thanks for Registration </h3>
<table border="1" style="color:blue;">
    <tr>
        <th>CourseID</th>
        <th>Course Name</th>
        <th>Fees</th>
    </tr>
    @foreach(var item in Model)
     {
        <tr>
            <td>@item.CourseId</td>
            <td>@item.CourseName</td>
            <td>@item.Fees</td>
        </tr>
     }
</table>